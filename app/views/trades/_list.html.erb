<table class='table table-striped'>
  <thead class='thead-inverse'>
    <tr>
      <th>Buy</th>
      <th>Sell</th>
    </tr>
  </thead>
  <tbody>
    <% for trade in @trades %>
      <tr>
        <td>
          <%= trade.debit.name %>: <strong><%= trade.debit.quantity %></strong>
        </td>
        <td>
          <%= trade.credit.name %>: <strong><%= trade.credit.quantity %></strong>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
